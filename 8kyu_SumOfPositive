import java.util.*;

public class Positive{

  public static int sum(int[] arr){
    return Arrays.stream(arr).filter(V -> V>0).sum();
  }

}

public class Positive{

  public static int sum(int[] arr){
    int sum=0;
    for (int i = 0; i<arr.length; i++)
      if (arr[i]>0)
        sum+=arr[i];  
    return sum;
  }

}
