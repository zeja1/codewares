import static java.lang.Math.*;
public class NumberFun {
  public static long findNextSquare(long sq) {
        return (sq%sqrt(sq)==0) ? (long)pow(sqrt(sq)+1,2) : -1;
      
  }
}

import static java.lang.Math.*;

public class NumberFun {
  public static long findNextSquare(long sq) {
      
    if (sq%sqrt(sq)==0){
        return (long)pow((sqrt(sq)+1),2);
	    }else
		    return -1;
  }
}
